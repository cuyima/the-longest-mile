{
  "compilerOptions": {
    "target": "es2016",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "moduleResolution": "node16",
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "dist"
  }
}
